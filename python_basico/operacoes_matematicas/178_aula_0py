produtos = [
    {'nome': 'Produto 5', 'preco': 10.01 },
    {'nome': 'Produto 1', 'preco': 22.32},
    {'nome': 'Produto 3', 'preco': 10.11 },
    {'nome': 'Produto 2', 'preco': 105.87},
    {'nome': 'Produto 4', 'preco': 69.90},
]

print(round(sum(p['preco'] for p in produtos),2))







